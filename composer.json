{
    "name": "deva7mad/laravel-cybersource",
    "description": "This package wraps the Cybersource SOAP API in a convenient, easy to use package for Laravel.",
    "type": "library",
    "keywords": [
        "cybersource",
        "integration",
        "payments",
        "visa",
        "laravel",
        "laravel-cybersource",
        "cybersource payments laravel"
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": ">=5.4",
        "besimple/soap-client": "^0.2.6"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.0",
        "mockery/mockery": "^1.0",
        "satooshi/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "JustGeeky\\LaravelCybersource\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JustGeeky\\LaravelCybersource\\tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmad Eleknany",
            "email": "dev.elknany@gmail.com",
            "role": "Software Engineer"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "JustGeeky\\LaravelCybersource\\Providers\\LaravelCybersourceServiceProvider"
            ],
            "aliases": {
                "Cybersource": "JustGeeky\\LaravelCybersource\\Facades\\Cybersource"
            }
        }
    },
    "minimum-stability": "dev"
}
